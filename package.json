{
  "name": "node-frontend",
  "version": "1.0.0",
  "description": "Web App with Node.js",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/js/app.js",
    "build": "node build.js",
    "wc-build": "webpack -p",
    "w-build": "webpack --mode development --entry ./src/js/app.js --output ./dist/bundle.js",
    "w-build:prod": "webpack --mode production --entry ./src/js/app.js --output ./dist/bundle.js",
    "wds-build": "webpack-dev-server --mode development --entry ./src/js/app.js --output ./dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Corars/node-frontend.git"
  },
  "keywords": [
    "node",
    "express",
    "webapp"
  ],
  "author": "Mehmet ALTINEL (med177@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Corars/node-frontend/issues"
  },
  "homepage": "https://github.com/Corars/node-frontend#readme",
  "dependencies": {
    "express": "^4.16.3",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.0.6",
    "nodemon": "^1.17.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  }
}